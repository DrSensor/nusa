enable lazy;
disable unicode;

#example "ctrl" "Alt" "Shift" "super" ...
let mod = [ascii_alpha]+;

#example 'A' '1' 'z' ...
let key = [ascii_alnum];

#example "ArrowDown" "enter" "Escape" ...
let sym = [ascii_alpha]+;

'[' (
  :cmb(
    ( :mod(mod) '-' )?
    ( :key(key|sym) '-'? )+
  ) #--------------key combination

  (
    :seq(' '+) #---key sequence
  |
    :or(',') #-----key alias
  )?
)+ ']'
